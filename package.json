{
  "author": "carathorys <gallayb@gmail.com>",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "saxophone-ts": "^0.0.0-p5",
    "xml-js": "^1.6.11",
    "xmlbuilder2": "^1.4.1",
    "zone.js": "^0.10.2"
  },
  "description": "",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-typescript": "^3.0.0",
    "@types/jest": "^25.1.3",
    "@types/readable-stream": "^2.3.5",
    "@types/node": "^13.7.4",
    "colors": "^1.4.0",
    "commitizen": "^4.0.3",
    "core-js": "^3.6.4",
    "coveralls": "^3.0.9",
    "cross-env": "^7.0.0",
    "cz-conventional-changelog": "^3.1.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-config": "^25.1.0",
    "jest-fetch-mock": "^3.0.1",
    "lint-staged": "^10.0.7",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.19.1",
    "prompt": "^1.0.0",
    "replace-in-file": "^5.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.1",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "semantic-release": "^17.0.4",
    "shelljs": "^0.8.3",
    "travis-deploy-once": "^5.0.11",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typedoc": "^0.16.10",
    "typescript": "^3.8.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jestConfig.js"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    },
    "_coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    }
  },
  "keywords": [
    "rtorrent",
    "xmlrpc",
    "typescript"
  ],
  "license": "MIT",
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "dist/rtorrent-tslib.umd.js",
  "module": "dist/rtorrent-tslib.es5.js",
  "name": "rtorrent-tslib",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && npm run docs",
    "commit": "git-cz",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "docs": "typedoc --out docs --target es6 --theme minimal --mode file src --exclude **/*.test.ts --excludeExternals --excludePrivate --excludeNotExported",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "precommit": "lint-staged",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "test:watch": "jest --coverage --watch",
    "travis-deploy-once": "travis-deploy-once"
  },
  "typings": "dist/types/rtorrent-tslib.d.ts",
  "version": "0.0.9"
}
